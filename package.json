{
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node scripts/runMigrations.js run --force",
    "migrate:dry-run": "node scripts/runMigrations.js run --dry-run",
    "migrate:status": "node scripts/runMigrations.js status",
    "migrate:rollback": "node scripts/runMigrations.js rollback",
    "db:backup": "node scripts/backupDatabase.js",
    "db:backup:quick": "node scripts/backupDatabase.js quick",
    "db:backup:full": "node scripts/backupDatabase.js full",
    "db:backup:list": "node scripts/backupDatabase.js list",
    "db:backup:recommend": "node scripts/backupDatabase.js recommend",
    "db:restore": "node scripts/restoreDatabase.js",
    "phase1:backup": "npm run db:backup:quick",
    "phase1:migrate": "npm run db:backup:quick && npm run migrate",
    "phase1:check": "npm run migrate:status && node scripts/validatePhase1.js",
    "phase1:rollback": "npm run migrate:rollback 002_standardize_likes_arrays && npm run migrate:rollback 001_consolidate_photo_event_fields"
